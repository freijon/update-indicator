                            <property name="visible">True</property>
                            <property name="can_focus">False</property>
                            <property name="margin_right">5</property>
                            <property name="margin_bottom">5</property>
                            <items>
                              <item translatable="yes">Show updates in update-manager</item>
                              <item translatable="yes">Upgrade using update-manager</item>
                              <item translatable="yes">Upgrade in background</item>
                            </items>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child type="label">
                      <object class="GtkLabel" id="label2">
